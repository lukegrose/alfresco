          <activiti:string>logger.log("end event called");
          if(bpm_package.children){
var agenda = bpm_package.children[0];
var status = execution.getVariable("wfstatus");
cio = people.getPerson("jon");
convenor = people.getPerson("robert");
if(status=="start"){
var mail = actions.create("mail");   
mail.parameters.to = cio.properties["cm:email"];   
mail.parameters.subject = "ITAC Agenda Review workflow has been cancelled";   
mail.parameters.from =initiator.properties.email;   
mail.parameters.body="ITAC Agenda Review workflow has been cancelled for doc "+agenda.name;
mail.execute(agenda); 
var mail = actions.create("mail");   
mail.parameters.to = convenor.properties["cm:email"];   
mail.parameters.subject = "ITAC Agenda Review workflow has been cancelled";   
mail.parameters.from =initiator.properties.email;   
mail.parameters.body="ITAC Agenda Review workflow has been cancelled for doc "+agenda.name;
mail.execute(agenda); 
}
}</activiti:string>
